{
  "name": "elmo-frontend-demo",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend demo for Elmo with React, Redux and Typescript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --debug --devtool cheap-module-source-map --output-pathinfo --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "build": "webpack -p --progress --colors",
    "format": "prettier --write '**/*.{md,js,json,ts,tsx}'"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "css"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.ts$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx$": "babel-jest"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "2.2.3",
    "@types/history": "4.6.2",
    "@types/node": "8.0.53",
    "@types/react": "^16.0.25",
    "@types/react-dom": "16.0.3",
    "@types/react-redux": "5.0.13",
    "@types/react-router": "4.0.18",
    "@types/redux-actions": "2.2.2",
    "@types/redux-saga": "^0.10.5",
    "@types/tapable": "^0.2.5",
    "@types/webpack": "3.8.1",
    "@types/webpack-env": "1.13.2",
    "awesome-typescript-loader": "^3.4.0",
    "babel-cli": "7.0.0-beta.3",
    "babel-core": "7.0.0-beta.3",
    "babel-jest": "^23.1.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "postcss": "^6.0.14",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.2.1",
    "react-hot-loader": "^3.1.3",
    "redux-devtools-extension": "^2.13.2",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^0.19.0",
    "typescript": "^3.9.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.4",
    "@babel/plugin-syntax-decorators": "7.0.0-beta.4",
    "@babel/preset-env": "7.0.0-beta.4",
    "@babel/preset-react": "7.0.0-beta.4",
    "@babel/preset-typescript": "7.0.0-beta.4",
    "@material-ui/core": "3.9.3",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "axios": "^0.19.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "7.0.0-beta.3",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "classnames": "^2.2.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^23",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-saga": "^0.16.0"
  }
}
